<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.project_flow_android">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name=".di.ProjectFlowApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/Theme.ProjectFlow_Android">
        <activity android:name=".ui.sign.ui.FinishSignActivity" />
        <activity android:name=".ui.sign.ui.CertificationActivity" />
        <activity android:name=".ui.sign.ui.EditPasswordActivity" />
        <activity android:name=".ui.sign.ui.DashBoardActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.chat.ChatActivity" />

        <activity android:name=".ui.sign.ui.RegisterActivity" />
        <activity android:name=".ui.sign.ui.LoginActivity" />
        <activity android:name=".ui.main.MainActivity" />
    </application>

</manifest>